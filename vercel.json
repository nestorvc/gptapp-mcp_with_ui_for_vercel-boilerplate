{
  "version": 2,
  "builds": [
    {
      "src": "build/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/mcp",
      "dest": "build/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "build/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

